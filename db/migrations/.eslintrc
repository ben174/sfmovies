{
  extends: "lob/migrations",
  parserOptions: {
    "ecmaVersion": 2018
  },
  rules: {
    "semi": 0,
    "comma-spacing": 0,
    "no-extra-parens": 0,
    "no-shadow": 0,
    "prefer-const": 0,
    "no-var": 0,
    "prefer-template": 0,
    "arrow-parens": 0,
    "no-extra-semi": 0,
    "no-unreachable": 0,
    "no-multiple-empty-lines": 0,
    "no-useless-concat": 0,
    "object-shorthand": 0,
    "prefer-reflect": 0,
    "no-arrow-condition": 0,
    "no-lonely-if": 0,
    "no-redeclare": 0,
    "prefer-arrow-callback": 0,
    "quotes": ["error", "single", { "allowTemplateLiterals": true }]
  }
}
